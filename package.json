{
    "name": "podman-status-monitor",
    "displayName": "Podman Status Monitor",
    "description": "A VSCode extension to monitor the status of Podman machines.",
    "version": "0.0.1",
    "publisher": "karmakars",
    "engines": {
        "vscode": "^1.95.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "podman.checkStatus",
                "title": "Check podman machine status"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "typescript": "^5.x",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "compile": "^0.0.2"
    }
}
