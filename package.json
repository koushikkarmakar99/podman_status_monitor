{
    "name": "podman-status-monitor",
    "displayName": "Podman Status Monitor",
    "description": "A VSCode extension to monitor the status of Podman machines.",
    "version": "1.0.0",
    "publisher": "karmakars",
    "engines": {
        "vscode": "^1.106.2"
    },
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/koushikkarmakar99/podman_status_monitor"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "podman",
        "podman status",
        "podman status monitor",
        "podman machine",
        "podman machine monitor"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "podman.checkStatus",
                "title": "Check podman machine status"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "@types/vscode": "^1.106.2",
        "typescript": "^5.x"
    },
    "dependencies": {
        "compile": "^0.0.2"
    }
}
